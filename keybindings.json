[
    // move cursro in editor
    {"key": "alt+i", "command": "cursorUp", "when": "editorTextFocus"},
    {"key": "alt+j", "command": "cursorLeft", "when": "editorTextFocus"},
    {"key": "alt+k", "command": "cursorDown", "when": "editorTextFocus"},
    {"key": "alt+l", "command": "cursorRight", "when": "editorTextFocus"},
    {"key": "alt+shift+j", "command": "cursorWordLeftSelect", "when": "textInputFocus"},
    {"key": "alt+shift+l", "command": "cursorWordRightSelect", "when": "textInputFocus"},
    {"key": "alt+shift+i", "command": "cursorUpSelect", "when": "textInputFocus"},
    {"key": "alt+shift+k", "command": "cursorDownSelect", "when": "textInputFocus"},
    {"key": "alt+;", "command": "cursorEnd", "when": "editorTextFocus"},
    {"key": "alt+h", "command": "cursorHome", "when": "editorTextFocus"},

    // move cursors in terminal
    {"key": "alt+i", "command": "workbench.action.terminal.sendSequence", "args": {"text": "\u001b[A"}, "when": "terminalFocus"},
    {"key": "alt+k", "command": "workbench.action.terminal.sendSequence", "args": {"text": "\u001b[B"}, "when": "terminalFocus"},
    {"key": "alt+j", "command": "workbench.action.terminal.sendSequence", "args": {"text": "\u001b[D"}, "when": "terminalFocus"},
    {"key": "alt+l", "command": "workbench.action.terminal.sendSequence", "args": {"text": "\u001b[C"}, "when": "terminalFocus"},

    // Line Operationsi
    {"key": "alt+u", "command": "editor.action.moveLinesUpAction", "when": "textInputFocus"},
    {"key": "alt+m", "command": "editor.action.moveLinesDownAction", "when": "textInputFocus"},
    {"key": "alt+o", "command": "scrollLineUp", "when": "editorTextFocus"},
    {"key": "alt+.", "command": "scrollLineDown", "when": "editorTextFocus"},
    {"key": "alt+n", "command": "editor.action.copyLinesDownAction", "when": "editorTextFocus"},
    {"key": "alt+enter", "command": "editor.action.insertLineBefore", "when": "editorTextFocus"},
    {"key": "ctrl+enter", "command": "editor.action.insertLineAfter", "when": "editorTextFocus"},
    {"key": "alt+d", "command": "editor.action.deleteLines", "when": "editorTextFocus && !editorReadonly"},
    {"key": "alt+q", "command": "cph.runTestCases"},
    {"key": "alt+f","command": "workbench.action.nextEditor"},
    {"key": "alt+a","command": "workbench.action.previousEditor"},

    // Terminal Operations
    {"key": "ctrl+shift+j", "command": "workbench.action.terminal.new"},
    {"key": "ctrl+alt+k", "command": "workbench.action.terminal.kill", "when": "terminalFocus"},
    {"key": "ctrl+tab", "command": "workbench.action.terminal.focusNext", "when": "terminalFocus"},
    {"key": "ctrl+shift+k","command": "workbench.action.terminal.kill","when": "terminalFocus"},
    {"key": "ctrl+alt+k","command": "workbench.action.terminal.killAll","when": "terminalFocus"}
]